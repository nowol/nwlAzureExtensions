{
  "manifestVersion": 1,
  "id": "nwl-pull-requests-hub-dev",
  "publisher": "ADrouinPerson",
  "version": "1.0.41",
  "name": "Pull Requests Hub",
  "description": "An Azure Devops extension to display every active pull requests of a project.",
  "public": false,
  "categories": [
    "Azure Repos"
  ],
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "icons": {
    "default": "src/PullRequests/images/logo.png"
  },
  "tags": [
    "pull request",
    "git",
    "darkmode"
  ],
  "content": {
    "details": {
      "path": "src/PullRequests/overview.md"
    },
    "license": {
      "path": "LICENSE.md"
    }
  },
  "screenshots": [
    {
      "path": "src/PullRequests/images/PRHUI.png"
    }
  ],
  "links": {
    "home": {
      "uri": "https://github.com/nowol/nwlAzureExtensions"
    },
    "support": {
      "url": "https://github.com/nowol/nwlAzureExtensions"
    }
  },
  "contributions": [
    {
      "id": "pull-requests-hub",
      "type": "ms.vss-web.hub",
      "description": "blabla.",
      "targets": [
        "ms.vss-code-web.code-hub-group"
      ],
      "properties": {
        "name": "Pull Requests Hub",
        "order": 100,
        "uri": "dist/PullRequests/index.html",
        "icon": "src/PullRequests/images/menuicon.png"
      }
    }
  ],
  "scopes": [
    "vso.code"
  ],
  "files": [
    {
      "path": "static",
      "addressable": true
    },
    {
      "path": "dist",
      "addressable": true
    },
    {
      "path": "src/PullRequests/images/menuicon.png",
      "addressable": true
    },
    {
      "path": "src/PullRequests/images/PRHUI.png",
      "addressable": true,
      "packagePath": "images/PRHUI.png"
    }
  ]
}